# Example Terraform variables file
# Copy this to terraform.tfvars and update with your values

# AWS Configuration
aws_region = "us-east-1"
project_name = "corelight-api"

# EC2 Configuration
instance_type = "t3.micro"
key_pair_name = "your-key-pair-name"

# GitHub Repository Configuration
github_repo = "https://github.com/yourusername/your-repo.git"
github_branch = "main"

# Security Configuration
ssh_cidr_blocks = ["YOUR.IP.ADDRESS/32"]  # Replace with your IP for better security
dev_port_cidr_blocks = ["YOUR.IP.ADDRESS/32"]  # Replace with your IP for better security
